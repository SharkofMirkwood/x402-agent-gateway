{
  "name": "@x402-agent-gateway/client",
  "version": "1.0.0",
  "description": "Frontend SDK for x402-native agent and tool orchestration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "!dist/**/__tests__/**",
    "!dist/**/*.test.*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "peerDependenciesMeta": {
    "@solana/web3.js": {
      "optional": false
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "x402",
    "solana",
    "payment",
    "ai",
    "tools",
    "client"
  ],
  "license": "MIT",
  "dependencies": {
    "@solana/web3.js": "^1.87.0",
    "axios": "^1.6.0",
    "buffer": "^6.0.3",
    "x402-solana": "^0.1.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  }
}
